#import json
#import mem
#import os
#import io

test_string :: #embed_str "../test.json";
//test_string :: "{\"v\": \"\"}";

main :: fn() -> i32 {
    ga := mem.create_generic_allocator();
    obj := json.parse(test_string, &ga);
	json.print_whitespace = true;
	str := json.print(obj, &ga);
	io.println("%", str);


    return 0;
}


